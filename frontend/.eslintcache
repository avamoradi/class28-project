roducts(\n        keyword,\n        pageNumber,\n        location,\n        minPrice,\n        maxPrice,\n        color,\n        sort\n      )\n    )\n  }, [dispatch, keyword, pageNumber, location, minPrice, maxPrice, color, sort])\n\n  return (\n    <>\n      <Meta />\n      <HomeSlider />\n      <AboutGalileo />\n      {!keyword || !location || !minPrice || !maxPrice || !color || !sort ? (\n        <ProductCarousel />\n      ) : (\n        <Link to='/' className='btn btn-light'>\n          Go Back\n        </Link>\n      )}\n      <h1 id='latest-art'>Latest Art</h1>\n      {loading ? (\n        <Loader />\n      ) : error ? (\n        <Message variant='danger'>{error}</Message>\n      ) : (\n        <>\n          <Row>\n            <Filtering\n              location={location}\n              setLocation={setLocation}\n              color={color}\n              setColor={setColor}\n              minPrice={minPrice}\n              setMinPrice={setMinPrice}\n              maxPrice={maxPrice}\n              setMaxPrice={setMaxPrice}\n            />\n          </Row>\n          <Row>\n            {/* <Route render={({ history }) => <Sorting history={history} />} /> */}\n            <Sorting sort={sort} setSort={setSort} />\n          </Row>\n          <Row>\n            {products.map((product) => (\n              <Col key={product._id} sm={12} md={6} lg={4} xl={3}>\n                <Product product={product} />\n              </Col>\n            ))}\n          </Row>\n          <Paginate\n            pages={pages}\n            page={page}\n            keyword={keyword ? keyword : ''}\n          />\n        </>\n      )}\n    </>\n  )\n}\n\nexport default HomeScreen\n","/Users/hackyourfuture/Documents/class28-project/frontend/src/actions/orderActions.js",[],"/Users/hackyourfuture/Documents/class28-project/frontend/src/index.js",[],"/Users/hackyourfuture/Documents/class28-project/frontend/src/store.js",[],"/Users/hackyourfuture/Documents/class28-project/frontend/src/red